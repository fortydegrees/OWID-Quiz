{
  "name": "OWID-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/cheerio": "^0.22.15",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.25",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "mobx": "5.15.2",
    "mobx-react-lite": "1.5.2",
    "mobx-state-tree": "3.15.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "typescript": "3.7.5"
  },
  "scripts": {
    "start": "npm install -g serve && serve -s build",
    "build": "yarn run build:css && react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/css/index.src.css -o src/css/index.css",
    "watch:css": "postcss src/css/index.src.css -o src/css/index.css -w",
    "dev": "concurrently \"yarn watch:css --silent\" \"yarn start --silent\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.0.5",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.7.4",
    "concurrently": "5.0.2",
    "cssnano": "4.1.10",
    "postcss-cli": "7.1.0",
    "tailwindcss": "1.1.4"
  }
}
